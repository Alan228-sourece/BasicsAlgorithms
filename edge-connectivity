// Vishka.cpp : Этот файл содержит функцию "main". Здесь начинается и заканчивается выполнение программы.
//

#include <iostream>
#include <algorithm>
#include <cmath>
#include <vector>
#include <stack>
#include <queue>
using namespace std;
bool check(int x1,int y1,int x2,int y2) {
	if (abs(x1 - x2) == abs(y1 - y2)) {
		return 1;
	}
	return 0;
}
int main()
{
	int x1, x2, y1, y2;
	cin >> x1 >> y1 >> x2 >> y2;
	int color1 = (x1 + y1) % 2;
	int color2 = (x2 + y2) % 2;
	if (x1 == x2 && y1 == y2) {
		cout << 0;
	}
	else {
		vector<int> x;
		vector<int>y;
		int count = 0;
		if (color2 == 0) { cout << -1; }
		else {
			if (color1 == 0) {
				bool da = 0;
				for (int i = -2; i <= 2; i++)
				{
					int d = 1233123;
					if (i != 0) {
						if (abs(i) == 1) {
							d = 2;
							if (x1 + i == x2 && y1 + d == y2) {
								da = 1;
								cout << 1<<endl;
								cout << x2 << " " << y2;
							}
							d = -2;
							if (x1 + i == x2 && y1 + d == y2) {
								da = 1;
								cout << 1 << endl;
								cout << x2 << " " << y2;
							}
						}
						else {
							d = 1;
							if (x1 + i == x2 && y1 + d == y2) {
								da = 1;
								cout << 1 << endl;
								cout << x2 << " " << y2;
							}
							d = -1;
							if (x1 + i == x2 && y1 + d == y2) {
								da = 1;
								cout << 1 << endl;
								cout << x2 << " " << y2;
							}
						}
					}
				}
				if (da) {
					
				}
			
				else {
					for (int i = -2; i <= 2; i++)
					{
						int d = 1233123;
						if (i != 0) {
							if (abs(i) == 1) {
								d = 2;
								if (check(x1 + i, y1 + d, x2, y2)) {
									count++;

									x1 = x1 + i;
									y1 = y1 + d;
									x.push_back(x1);
									y.push_back(y1);
									break;

								}
								d = -2;
								if (check(x1 + i, y1 + d, x2, y2)) {
									count++;

									x1 = x1 + i;
									y1 = y1 + d;
									x.push_back(x1);
									y.push_back(y1);
									break;

								}
							}
							else {
								d = 1;
								if (check(x1 + i, y1 + d, x2, y2)) {
									count++;

									x1 = x1 + i;
									y1 = y1 + d;
									x.push_back(x1);
									y.push_back(y1);
									break;

								}
								d = -1;
								if (check(x1 + i, y1 + d, x2, y2)) {
									count++;

									x1 = x1 + i;
									y1 = y1 + d;
									x.push_back(x1);
									y.push_back(y1);
									break;

								}
							}



						}
					}

					if (count == 0) {
						x1 = x1 + 1;
						y1 = y1 + 2;
						x.push_back(x1);
						y.push_back(y1);
					}
					if (x1 == x2 && y1 == y2) {
						cout << 1 << endl;
						cout << x[0] << " " << y[0];
					}
					else if ((x1 - x2) == abs(y2 - y1)) {
						cout <<1 + 1 << endl;
						
						cout << x[0] << " " << y[0] << " ";
						cout << x2 << " " << y2;
					}

					else {
						cout << 1 + 2 << endl;
					
						int dx = x2 - x1;
						int dy = y2 - y1;
						int l = (dx + dy) / 2;
						cout << x1 + l << " " << y1 + l<<" ";
						cout << x2 << " " << y2;
					}
				}
			}

			
			else {
				if ((x1 - x2) == abs(y2 - y1)) {
					cout << 1 << endl;
					cout << x2 << " " << y2;
				}
				else {
					cout << 2 << endl;
					int dx = x2 - x1;
					int dy = y2 - y1;
					int l = (dx + dy) / 2;
					cout << x1 + l << " " << y1 + l<<" ";
					cout << x2 << " " << y2;
				}
			}
		}

	}

}
